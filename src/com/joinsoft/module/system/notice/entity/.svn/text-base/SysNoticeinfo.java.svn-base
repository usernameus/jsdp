package com.joinsoft.module.system.notice.entity;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;
/**
 * 通知消息
 * @author scy
 * @since  2018年3月30日
 */
public class SysNoticeinfo{
	private java.lang.String sid;
	private java.lang.String orgid;
	private java.lang.String userid;
	private java.lang.String ntctitle;
	private java.lang.String describe;
	private java.lang.String state;
	private java.lang.String ntctype;
	private java.lang.String usertype;
	private java.lang.String releasedate;
	private java.lang.String rectime;
	private java.lang.String remark;


	public void setSid(java.lang.String value) {
		this.sid = value;
	}
	
	public java.lang.String getSid() {
		return this.sid;
	}
	public void setOrgid(java.lang.String value) {
		this.orgid = value;
	}
	
	public java.lang.String getOrgid() {
		return this.orgid;
	}
	public void setUserid(java.lang.String value) {
		this.userid = value;
	}
	
	public java.lang.String getUserid() {
		return this.userid;
	}
	public void setNtctitle(java.lang.String value) {
		this.ntctitle = value;
	}
	
	public java.lang.String getNtctitle() {
		return this.ntctitle;
	}
	public void setDescribe(java.lang.String value) {
		this.describe = value;
	}
	
	public java.lang.String getDescribe() {
		return this.describe;
	}
	public void setState(java.lang.String value) {
		this.state = value;
	}
	
	public java.lang.String getState() {
		return this.state;
	}
	public void setNtctype(java.lang.String value) {
		this.ntctype = value;
	}
	
	public java.lang.String getNtctype() {
		return this.ntctype;
	}
	public void setUsertype(java.lang.String value) {
		this.usertype = value;
	}
	
	public java.lang.String getUsertype() {
		return this.usertype;
	}
	public void setReleasedate(java.lang.String value) {
		this.releasedate = value;
	}
	
	public java.lang.String getReleasedate() {
		return this.releasedate;
	}
	public void setRectime(java.lang.String value) {
		this.rectime = value;
	}
	
	public java.lang.String getRectime() {
		return this.rectime;
	}
	public void setRemark(java.lang.String value) {
		this.remark = value;
	}
	
	public java.lang.String getRemark() {
		return this.remark;
	}


	public String toString() {
		return new ToStringBuilder(this,ToStringStyle.MULTI_LINE_STYLE)
			.append("Sid",getSid())
			.append("Orgid",getOrgid())
			.append("Userid",getUserid())
			.append("Ntctitle",getNtctitle())
			.append("Describe",getDescribe())
			.append("State",getState())
			.append("Ntctype",getNtctype())
			.append("Usertype",getUsertype())
			.append("Releasedate",getReleasedate())
			.append("Rectime",getRectime())
			.append("Remark",getRemark())
			.toString();
	}
}

